{
  "name": "prisma-nextjs-graphql",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"tailwindcss --input ./styles/root.css --output ./styles/output.css --watch\"",
    "build": "tailwindcss ./styles/root.css --output ./styles/output.css && yarn generate:prisma && next build",
    "start": "next start",
    "lint": "next lint",
    "relay": "relay-compiler",
    "relay-watch": "yarn relay-compiler --watch",
    "codegen": "graphql-codegen --config codegen.yml",
    "seed": "dotenv -e .env.local -- prisma db seed",
    "migrate:dev": "dotenv -e .env.local -- prisma migrate dev --schema=./server/prisma/schema.prisma",
    "migrate:deploy": "dotenv -e .env.local -- prisma migrate deploy --schema=./server/prisma/schema.prisma",
    "generate:nexus": "ts-node --transpile-only ./server/schema",
    "generate:prisma": "prisma generate --schema=./server/prisma/schema.prisma",
    "generate:server": "yarn generate:nexus && yarn generate:prisma",
    "generate:client": "yarn codegen && yarn relay",
    "generate:all": "yarn generate:server && yarn generate:client"
  },
  "dependencies": {
    "@apollo/server": "^4.3.3",
    "@formatjs/intl-localematcher": "^0.2.32",
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^3.0.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@next/font": "v13.1.7-canary.6",
    "@prisma/client": "4.9.0",
    "@types/node": "18.11.19",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "client-only": "^0.0.1",
    "graphql": "^16.6.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-middleware": "^6.1.33",
    "negotiator": "^0.6.3",
    "next": "^13.1.6",
    "nexus": "^1.3.0",
    "prisma": "^4.9.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-relay": "^14.1.0",
    "relay-runtime": "^14.1.0",
    "server-only": "^0.0.1",
    "tiny-invariant": "^1.3.1",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/negotiator": "^0.6.1",
    "@types/react-relay": "^14.1.3",
    "@types/relay-test-utils": "^14.1.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "autoprefixer": "^10.4.13",
    "clsx": "^1.2.1",
    "concurrently": "^7.6.0",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.33.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "flatten-tailwindcss-theme": "^1.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "relay-compiler": "^14.1.0",
    "relay-compiler-language-typescript": "^15.0.1",
    "relay-config": "^12.0.1",
    "tailwind-merge": "^1.9.1",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1"
  },
  "prisma": {
    "seed": "ts-node --transpile-only ./server/prisma/seed.ts"
  }
}
