{
  "name": "prisma-nextjs-graphql",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed": "dotenv -e .env.local -- prisma db seed",
    "migrate:dev": "dotenv -e .env.local -- prisma migrate dev --schema=./server/prisma/schema.prisma",
    "migrate:deploy": "dotenv -e .env.local -- prisma migrate deploy --schema=./server/prisma/schema.prisma",
    "generate:nexus": "ts-node --transpile-only ./server/schema",
    "generate:prisma": "prisma generate --schema=./server/prisma/schema.prisma",
    "generate:server": "yarn generate:nexus && yarn generate:prisma"
  },
  "dependencies": {
    "@apollo/server": "^4.3.3",
    "@next/font": "13.1.6",
    "@prisma/client": "4.9.0",
    "@types/node": "18.11.19",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "graphql": "^16.6.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-middleware": "^6.1.33",
    "next": "13.1.6",
    "nexus": "^1.3.0",
    "prisma": "^4.9.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tiny-invariant": "^1.3.1",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@types/graphql-iso-date": "^3.4.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.33.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1"
  },
  "prisma": {
    "seed": "ts-node --transpile-only ./server/prisma/seed.ts"
  }
}
