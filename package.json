{
  "name": "prisma-nextjs-graphql",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"tailwindcss --input ./styles/root.css --output ./styles/output.css --watch\"",
    "build": "tailwindcss ./styles/root.css --output ./styles/output.css && yarn generate:prisma && next build",
    "start": "next start",
    "lint": "next lint",
    "seed": "dotenv -e .env.local -- prisma db seed",
    "migrate:dev": "dotenv -e .env.local -- prisma migrate dev",
    "migrate:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "generate:nexus": "ts-node --transpile-only ./server/schema",
    "generate:prisma": "prisma generate",
    "generate:server": "yarn generate:nexus && yarn generate:prisma"
  },
  "dependencies": {
    "@apollo/server": "^4.3.3",
    "@formatjs/intl-localematcher": "^0.2.32",
    "@next/font": "v13.1.7-canary.6",
    "@prisma/client": "4.10.1",
    "@types/node": "18.13.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "client-only": "^0.0.1",
    "graphql": "^16.6.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-middleware": "^6.1.33",
    "negotiator": "^0.6.3",
    "next": "^13.1.6",
    "nexus": "^1.3.0",
    "prisma": "^4.10.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "tiny-invariant": "^1.3.1",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/negotiator": "^0.6.1",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "autoprefixer": "^10.4.13",
    "clsx": "^1.2.1",
    "concurrently": "^7.6.0",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.34.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "flatten-tailwindcss-theme": "^1.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "tailwind-merge": "^1.9.1",
    "tailwindcss": "^3.2.6",
    "ts-node": "^10.9.1"
  },
  "prisma": {
    "seed": "ts-node --transpile-only ./server/prisma/seed.ts"
  }
}
